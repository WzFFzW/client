{
  "name": "nohost",
  "version": "1.0.0",
  "description": "nohost的PC客户端，帮助PC用户设置nohost环境，安装后打开即可接入nohost",
  "main": "main.js",
  "scripts": {
    "lint": "eslint *.js app/*.js app/lib",
    "lintfix": "eslint --fix *.js app/*.js app/lib",
    "start": "gulp",
    "pack:win32": "gulp pack --platform win32 --arch ia32",
    "pack:win64": "gulp pack --platform win32 --arch x64",
    "pack:mac": "gulp pack --platform darwin --arch x64",
    "release:win32": "gulp release --platform win32 --arch ia32",
    "release:win64": "gulp release --platform win32 --arch x64"
  },
  "repository": {
    "type": "git",
    "url": "http://git.code.oa.com/imweb/nohost-client.git"
  },
  "keywords": [
    "imweb",
    "nohost",
    "whistle"
  ],
  "author": "avenwu <avenwu@tencent.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "electron": "1.7.9",
    "electron-packager": "^9.1.0",
    "electron-winstaller": "^2.6.3",
    "eslint": "^3.10.2",
    "eslint-config-imweb": "^0.2.4",
    "gulp": "^3.9.1"
  },
  "dependencies": {
    "eslint-plugin-react": "^7.5.1"
  }
}
