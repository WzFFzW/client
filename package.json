{
  "name": "nohost",
  "version": "1.0.0",
  "description": "nohost的PC客户端，帮助PC用户设置nohost环境，安装后打开即可接入nohost",
  "main": "main.js",
  "scripts": {
    "lint": "eslint app/*.js app/lib",
    "lintfix": "eslint --fix app/*.js app/lib",
    "start": "gulp",
    "build:win32": "gulp build --platform win32 --arch ia32",
    "build:win64": "gulp build --platform win32 --arch x64",
    "build:mac": "gulp build --platform darwin --arch x64"
  },
  "repository": {
    "type": "git",
    "url": "http://git.code.oa.com/imweb/nohost-client.git"
  },
  "keywords": [
    "imweb",
    "nohost",
    "whistle"
  ],
  "author": "avenwu <avenwu@tencent.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "electron": "1.7.9",
    "electron-builder": "^19.49.2",
    "eslint": "^3.10.2",
    "eslint-config-imweb": "^0.2.4",
    "gulp": "^3.9.1"
  }
}
